{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css|wasm|woff|woff2|ttf|otf)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "barber-shop-whitelabel",
          "appId": "1:257675957691:android:a2eb3db33f360085fba09e",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "barber-shop-whitelabel",
          "appId": "1:257675957691:ios:6ea1477d33f9711ffba09e",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "barber-shop-whitelabel",
          "configurations": {
            "android": "1:257675957691:android:a2eb3db33f360085fba09e",
            "ios": "1:257675957691:ios:6ea1477d33f9711ffba09e",
            "macos": "1:257675957691:ios:6ea1477d33f9711ffba09e",
            "web": "1:257675957691:web:3d27589443bf2418fba09e",
            "windows": "1:257675957691:web:c29f272aaa622f3dfba09e"
          }
        }
      },
      "macos": {
        "default": {
          "projectId": "barber-shop-whitelabel",
          "appId": "1:257675957691:ios:6ea1477d33f9711ffba09e",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
        }
      }
    }
  }
}
